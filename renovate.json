{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "enabled": true,
  "minimumReleaseAge": "14 days",
  "extends": [
    "github>elastic/renovate-config"
  ],
  "labels": [
    "dependencies",
    "v9.3",
    "v9.2",
    "v9.1",
    "v8.19",
    "v8.18",
    "v7.17"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true
    },
    {
      "matchDepTypes": [
        "devDependencies"
      ],
      "automerge": true
    },
    {
      "groupName": "Elastic EUI",
      "matchPackageNames": [
        "@elastic/eui",
        "@elastic/eui-theme-borealis"
      ],
      "labels": [
        "dependencies",
        "v9.3",
        "v9.2",
        "v9.1"
      ],
      "minimumReleaseAge": "3 days"
    },
    {
      "groupName": "Elastic EUI (8.x)",
      "matchPackageNames": [
        "@elastic/eui"
      ],
      "matchBaseBranches": [
        "v8.x"
      ],
      "labels": [
        "dependencies",
        "v8.19",
        "v8.18",
        "v7.17"
      ],
      "allowedVersions": "^107\\.x",
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "minimumReleaseAge": "3 days"
    }
  ]
}
