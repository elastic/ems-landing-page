{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>elastic/renovate-config"
  ],
  "labels": [
    "dependencies",
    "v8.18",
    "v8.17",
    "v8.16",
    "v7.17"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true
    },
    {
      "matchDepTypes": [
        "devDependencies"
      ],
      "automerge": true
    },
    {
      "matchPackagePatterns": [
        "elastic/eui"
      ],
      "groupName": "Elastic EUI"
    }
  ]
}
