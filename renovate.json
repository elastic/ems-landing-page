{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>elastic/renovate-config"
  ],
  "labels": [
    "dependencies",
    "v9.2",
    "v9.1",
    "v9.0",
    "v8.19",
    "v8.18",
    "v8.17",
    "v7.17"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true
    },
    {
      "matchDepTypes": [
        "devDependencies"
      ],
      "automerge": true
    },
    {
      "groupName": "Elastic EUI",
      "matchPackageNames": [
        "@elastic/eui",
        "@elastic/eui-theme-borealis"
      ],
      "labels": [
        "dependencies",
        "v9.2",
        "v9.1",
        "v9.0",
        "v8.19",
        "v8.18"
      ],
      "minimumReleaseAge": "3 days"
    },
  ]
}
