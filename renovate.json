{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>elastic/renovate-config"
  ],
  "labels": [
    "dependencies",
    "v8.18",
    "v8.17",
    "v7.17"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true
    },
    {
      "matchDepTypes": [
        "devDependencies"
      ],
      "automerge": true
    },
    {
      "groupName": "Elastic EUI",
      "matchPackageNames": [
        "/elastic/eui/"
      ]
    }
  ]
}
